{
  "name": "playwright",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "playwright-bdd": "^5.3.0"
  },
  "scripts": {
    "playwright": "npx playwright test --no-deps --workers=4 --project=chrome",
    "playwright:ui": "yarn playwright --ui",
    "playwright:headed": "yarn playwright --headed",
    "playwright:report": "yarn playwright && npx playwright show-report",
    "playwright:debug": "yarn playwright --debug",
    "playwright:codegen": "npx playwright codegen"
  }
}